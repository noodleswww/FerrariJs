{
  "name": "ferrarijs",
  "version": "1.0.0",
  "description": "javascript extension",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "mocha test/**/*.js -w --compilers js:babel/register ",
    "test:single": "istanbul cover -x *.test.js _mocha -- -R spec test/**/*.js --compilers js:babel/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noodleswww/jsExtension.git"
  },
  "keywords": [
    "javascript",
    "framework",
    "Array",
    "RegExp"
  ],
  "author": "noodleswww007 <vendetta0114@gmail.com> (https://github.com/noodleswww)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noodleswww/jsExtension/issues"
  },
  "homepage": "https://github.com/noodleswww/jsExtension#readme",
  "devDependencies": {
    "babel": "^5.8.21",
    "colors": "^1.1.2",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "1.0.2",
    "eslint-plugin-react": "3.11.2",
    "ghooks": "0.3.2",
    "istanbul": "0.4.2",
    "mocha": "2.4.5",
    "webpack": "1.12.14"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single"
    }
  }
}
